{
    "manifest_version" : 3 ,
    "name" : "Ecole directe customizer",
    "version" : "1.6",
    "description" : "Extension pour personnaliser Ecole directe",
    "content_scripts": [{
        "matches" : ["https://www.ecoledirecte.com/*"] ,
        "js" : ["background.js"],
        "run_at": "document_end"
    }],
    "action" : {
        "default_popup" : "popUp.html",
        "default_title" : "Ecole directe customizer"
    },
    "homepage_url": "https://github.com/The-Sheep-Noob/Extension-ecole-directe",
    "permissions": [
        "storage"    
      ],
    "web_accessible_resources": [{
        "matches": ["<all_urls>"],
        "resources": ["ecole_directe_emploi_du_temp.png","arrow_left.png","arrow_right.png","Icon/Logo.png","Icon/Menu_des_extensions.png","Icon/cone.png","Icon/extension-location.png","Icon/work-icon2.svg"]
    }],
    "icons": {
        "16": "Icon/Ecole_Directe_logo.png",
        "32": "Icon/Ecole_Directe_logo.png",
        "48": "Icon/Ecole_Directe_logo.png",
        "128": "Icon/Ecole_Directe_logo.png"
      }
}

